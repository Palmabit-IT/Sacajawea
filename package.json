{
  "name": "@palmabit/sacajawea",
  "version": "2.0.5",
  "description": "Manage your multi-language routes with Next.js, easily",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Palmabit-IT/Sacajawea.git"
  },
  "main": "dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "standard 'src/*' 'test/*'",
    "lint-fix": "standard --fix src/*",
    "build": "del dist/* && babel src -d dist",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint && npm run build",
    "test": "npm run testOnly",
    "testOnly": "jest \\.test.js --coverage",
    "testDev": "jest \\.test.js --coverage --watchAll",
    "testCI": "npm run test && cat coverage/lcov.info | coveralls"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-extended",
    "roots": [
      "test/",
      "dist/"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./test/setup.js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/config/**"
    ]
  },
  "dependencies": {
    "async": "^2.6.1",
    "babel-polyfill": "^6.26.0",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "next": ">=4",
    "react": "^16"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.5.2",
    "concurrently": "^3.5.0",
    "coveralls": "^3.0.1",
    "del-cli": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^20.0.4",
    "jest-extended": "^0.7.2",
    "next": "^4.1.4",
    "node-mocks-http": "^1.7.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "standard": "^10.0.2"
  },
  "author": "neomusic",
  "license": "MIT",
  "keywords": [
    "next",
    "next.js",
    "react",
    "route",
    "router",
    "routes",
    "routing",
    "multi-language"
  ],
  "bugs": {
    "url": "https://github.com/Palmabit-IT/Sacajawea/issues"
  },
  "homepage": "https://github.com/Palmabit-IT/Sacajawea#readme",
  "directories": {
    "test": "test"
  }
}
